                "exec": [
                  "pm.test(\"Login response has token\", function () {\r",
                  "    var jsonData = pm.response.json();\r",
                  "    pm.expect(jsonData.data.token).to.be.a(\"string\");\r",
                  "    pm.collectionVariables.set(\"token\", jsonData.data.token);\r",
                  "});"
                ],
                "type": "text/javascript",
                "packages": {}
              }
